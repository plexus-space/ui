{
  "name": "@plexus/ui-aerospace",
  "version": "0.1.0",
  "private": true,
  "description": "Monorepo for primitive-based aerospace component library with planetary visualization components for React Three Fiber",
  "workspaces": [
    "packages/cli",
    "playground"
  ],
  "scripts": {
    "dev": "npm run dev -w @plexusui/playground",
    "build": "npm run build -w @plexusui/cli",
    "build:cli": "npm run build -w @plexusui/cli",
    "publish": "npm run build:cli && npm publish -w @plexusui/cli --access public"
  },
  "keywords": [
    "monorepo",
    "aerospace",
    "ui",
    "components",
    "react",
    "three.js",
    "r3f",
    "react-three-fiber",
    "primitives",
    "planets",
    "visualization",
    "shadcn"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/plexus-space/ui.git"
  },
  "devDependencies": {
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@types/three": "^0.160.0",
    "typescript": "^5.4.5"
  }
}